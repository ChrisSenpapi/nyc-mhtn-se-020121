<h1> Add a new place! </h1>

<%= form_for @place  do |form_helper|%>
    <%= form_helper.label :name %>
    <%= form_helper.text_field :name %><br/>
    <%= form_helper.label :street_address %>
    <%= form_helper.text_field :street_address %><br/>
    <%= form_helper.label :description %>
    <%= form_helper.text_field :description %><br/>
    <%= form_helper.label :zip %>
    <%= form_helper.number_field :zip %><br/>
    <%= form_helper.submit "Done!" %>
<% end %>